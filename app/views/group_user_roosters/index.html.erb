<p id="notice"><%= notice %></p>

<h1>Group User Roosters</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Isadmin</th>
      <th>Group</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @group_user_roosters.each do |group_user_rooster| %>
      <tr>
        <td><%= group_user_rooster.user[:first_name] %> <%= group_user_rooster.user[:last_name] %></td>
        <td><%= group_user_rooster.isadmin %></td>
        <td><%= group_user_rooster.group[:name] %></td>
        <td><%= link_to 'Show', group_user_rooster %></td>
      </tr>
    <% end %>
  </tbody>
</table>

